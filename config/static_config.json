	"cropPreview": {
		"detectNumberFrames": 10
	},
	"languages": {
		"ger":	"Deutsch",	
		"eng":	"English",
		"unknown":	"-Unbekannt-"
	},
	"queue": {
		"max_convert_tasks": 1,
		"max_scan_tasks": 5,
		"max_extract_tasks": 1,
		"procNiceLevel": 15
	},
	"scanModules": {
		"video": {
			"fileExtensions": {
				"match":	["mkv", "mp4", "avi", "flv", "3gp", "m2ts", "m2v", "mov", "mpeg", "ts", "vob", "wmv", "mts"],
				"join":		["idx", "aac", "mkv", "mp4", "avi", "mp3", "srt", "m4a"]
			}
		},
		"rar": {
			"fileExtensions": {
				"match":	"rar",
				"join":		[],
				"group":	"/^r[\\d]+$/i"
			}
		}
	},
	"caching": {
		"minify": {
			"useToptalAPI":	false
		}
	}
}